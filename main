    state.antiAFK = on
    if on then
        setupAntiAFK()
        print("‚úÖ anti-afk enabled")
    end
end)

toggle("Auto Hit Resources", "üî®", function(on)
    state.autoHit = on
    if on then
        connections.autoHit = RunService.Heartbeat:Connect(hitResources)
    else
        if connections.autoHit then
            connections.autoHit:Disconnect()
        end
    end
end)

